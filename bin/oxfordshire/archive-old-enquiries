#!/usr/bin/env perl

use strict;
use warnings;
require 5.8.0;

BEGIN {
    use File::Basename qw(dirname);
    use File::Spec;
    my $d = dirname(File::Spec->rel2abs($0));
    require "$d/../../setenv.pl";
}

use FixMyStreet::Script::ArchiveOldEnquiries;

FixMyStreet::Script::ArchiveOldEnquiries::archive();
